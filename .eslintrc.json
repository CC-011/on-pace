{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "no-unused-vars": "error",
    "no-console": "error",
    "prefer-const": "error",
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "no-multiple-empty-lines": ["error", { "max": 1 }],
    "no-extra-semi": "error",
    "camelcase": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
